<launch>
  <!-- process events -->
  <node name="dvs_displayer" pkg="dvs_displayer" type="dvs_displayer_node" output="screen">
    <remap from="events" to="/dvs/events" />
    <remap from="event_image" to="event_image" />
  </node>

  <!-- display using rqt_image_view -->
  <node name="image_view" pkg="rqt_image_view" type="rqt_image_view">
  	<remap from="image" to="event_image"/>
  </node>

  <!-- rqt dynamic reconfigure-->
  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" />

</launch>
